[user]
	name = 
	email = 
[credential]
	helper = store
[gui]
	editor = code
[cola]
	spellcheck = false
[global]
[diff]
        tool = difftastic

[difftool]
        prompt = false

[difftool "difftastic"]
        cmd = difft "$LOCAL" "$REMOTE"
# Use a pager for large output, just like other git commands.
[pager]
        difftool = true
[merge]
	tool = meld
[alias]
	zap = !git add -A && git commit --allow-empty-message -m ''  && git push
	logg = !git log --pretty=oneline --graph --all --abbrev-commit
        dft = difftool
